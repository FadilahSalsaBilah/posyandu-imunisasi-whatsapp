<%- include("layout", { title: "Log WA", user, body: `
  <h1>Log Notifikasi WhatsApp</h1>

  <div class="card">
    <table>
      <thead>
        <tr>
          <th>ID</th><th>Type</th><th>Send At</th><th>Status</th>
          <th>Ortu</th><th>WA</th><th>Balita</th><th>Imunisasi</th><th>Error</th>
        </tr>
      </thead>
      <tbody>
        ${logs.map(l => `
          <tr>
            <td>${l.id}</td>
            <td>${l.type}</td>
            <td>${l.send_at}</td>
            <td>${l.status}</td>
            <td>${l.parent_name}</td>
            <td>${l.phone}</td>
            <td>${l.child_name}</td>
            <td>${l.imun_name}</td>
            <td>${l.last_error || ""}</td>
          </tr>
        `).join("")}
      </tbody>
    </table>
  </div>
` }) %>